{
    "name": "wizarr-backend",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/wizarr-backend/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nx/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "defaultConfiguration": "production",
            "options": {
                "target": "esnext",
                "platform": "node",
                "minify": false,
                "outputPath": "dist/apps/wizarr-backend",
                "metafile": true,
                "bundle": false,
                "inspect": false,
                "main": "apps/wizarr-backend/src/main.ts",
                "tsConfig": "apps/wizarr-backend/tsconfig.app.json",
                "generatePackageJson": true,
                "generateLockfile": true,
                "external": "none",
                "esbuildOptions": {
                    "sourcemap": true,
                    "outExtension": {
                        ".js": ".js"
                    }
                }
            },
            "configurations": {
                "development": {},
                "production": {
                    "esbuildOptions": {
                        "sourcemap": false,
                        "outExtension": {
                            ".js": ".js"
                        }
                    }
                }
            }
        },
        "serve": {
            "executor": "@nx/js:node",
            "defaultConfiguration": "development",
            "options": {
                "inspect": "inspect"
            },
            "configurations": {
                "development": {
                    "buildTarget": "wizarr-backend:build:development"
                },
                "production": {
                    "buildTarget": "wizarr-backend:build:production"
                }
            }
        }
    },
    "tags": []
}