{
    "name": "wizarr-backend-old",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "apps/wizarr-backend-old/wizarr_backend",
    "targets": {
        "lock": {
            "executor": "@nxlv/python:run-commands",
            "options": {
                "command": "poetry lock --no-update",
                "cwd": "apps/wizarr-backend-old",
                "color": true
            }
        },
        "add": {
            "executor": "@nxlv/python:add",
            "options": {}
        },
        "update": {
            "executor": "@nxlv/python:update",
            "options": {}
        },
        "remove": {
            "executor": "@nxlv/python:remove",
            "options": {}
        },
        "build": {
            "executor": "@nxlv/python:build",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/apps/wizarr-backend-old",
                "publish": false,
                "lockedVersions": true,
                "bundleLocalDependencies": true
            }
        },
        "serve": {
            "executor": "@nxlv/python:run-commands",
            "options": {
                "commands": [
                    "make-dir ../../database",
                    "poetry run flask run --debug"
                ],
                "cwd": "apps/wizarr-backend-old/wizarr_backend"
            }
        },
        "install": {
            "executor": "@nxlv/python:install",
            "options": {
                "silent": false,
                "args": "",
                "cacheDir": ".cache/pypoetry",
                "verbose": false,
                "debug": false
            }
        },
        "lint": {
            "executor": "@nxlv/python:flake8",
            "outputs": [
                "{workspaceRoot}/reports/apps/wizarr-backend-old/pylint.txt"
            ],
            "options": {
                "outputFile": "reports/apps/wizarr-backend-old/pylint.txt"
            }
        },
        "test": {
            "executor": "@nxlv/python:run-commands",
            "outputs": [
                "{workspaceRoot}/reports/apps/wizarr-backend-old/unittests",
                "{workspaceRoot}/coverage/apps/wizarr-backend-old"
            ],
            "options": {
                "command": "poetry run pytest tests/",
                "cwd": "apps/wizarr-backend-old"
            }
        }
    },
    "tags": []
}